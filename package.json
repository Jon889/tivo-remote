{
  "name": "tivo-remote",
  "version": "1.0.1",
  "description": "Control your TiVo DVR over the network",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src -d dist -D",
    "example": "node ./dist/example.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forty2/tivo-remote.git"
  },
  "keywords": [
    "TiVo",
    "DVR",
    "remote",
    "control",
    "home",
    "automation"
  ],
  "author": "Zach Bean <zb@forty2.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-plugin-syntax-function-bind": "^6.13.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-env": "^1.4.0"
  },
  "babel": {
    "sourceMaps": "inline",
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 6
          }
        }
      ]
    ],
    "plugins": [
      "syntax-function-bind",
      "transform-function-bind",
      "transform-object-rest-spread"
    ]
  },
  "engines": {
    "node": ">=6.0"
  },
  "bugs": {
    "url": "https://github.com/forty2/tivo-remote/issues"
  },
  "homepage": "https://github.com/forty2/tivo-remote#readme",
  "dependencies": {
    "debug": "^2.6.4",
    "mdns": "^2.3.3",
    "message-socket": "^1.0.3"
  }
}
